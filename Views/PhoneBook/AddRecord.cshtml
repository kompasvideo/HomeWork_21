@{
    ViewData["Title"] = "Записная книжка";
}
@model PhoneBook

    <div class="card card-outline-primary m-1 p-1">
        <div class="card-header bg-faded p-1 bg-primary text-white">
            Добавить запись
        </div>
        <div class="card-body">
            <div>
                Запись добавлена
            </div>
            <div>
                @Model.LastName
            </div>
            <div>
                @Model.FirstName
            </div>
            <div>
                @Model.ThreeName
            </div>
            <div>
                @Model.NumberPhone
            </div>
            <div>
                @Model.Address
            </div>
            <div>
                @Model.Description
            </div>
        </div>
    </div>
